{
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": false,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"incremental": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"paths": {
			"@/*": ["./*"],
			"@styles/*": ["./styles/*"],
			"@components/*": ["./components/*"],
			"@app-components/*": ["./app-components/*"],
			"@page-components/*": ["./page-components/*"],
			"@hooks/*": ["./hooks/*"],
			"@lib/*": ["./lib/*"],
			"@database/*": ["./prisma/*"],
			"@app-layout": ["./app-components/layout.jsx"],
			"@page-layout": ["./page-components/layout.jsx"],
			"@client": ["./prisma/client.ts"],
			"@logo": ["./branding/logos/main.jsx"],
			"@public/*": ["./public/*"],
			"@minio/*": ["./minio/*"]
		},
		"plugins": [
			{
				"name": "next"
			}
		],
		"strictNullChecks": true
	},
	"include": [
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx",
		".next/types/**/*.ts",
		"app/layout.jsx",
		"app/providers.jsx",
		"app/(MediBook)/layout.jsx",
		"src/app/(MediBook)/SubMenu.jsx",
		"src/app/(MediBook)/medibook/(Root)/announcements/[announcementId]/page.jsx",
		"src/app/(MediBook)/medibook/(Root)/announcements/page.jsx"
	],
	"exclude": ["node_modules"]
}
