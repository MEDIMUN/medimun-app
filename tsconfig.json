{
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": false,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"incremental": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"paths": {
			"@/*": ["./*"],
			"@styles/*": ["./styles/*"],
			"@components/*": ["./components/*"],
			"@app-components/*": ["./app-components/*"],
			"@page-components/*": ["./page-components/*"],
			"@hooks/*": ["./hooks/*"],
			"@lib/*": ["./lib/*"],
			"@database/*": ["./prisma/*"],
			"@app-layout": ["./app-components/layout.jsx"],
			"@page-layout": ["./page-components/layout.jsx"],
			"@client": ["./prisma/client.ts"],
			"@logo": ["./branding/logos/main.jsx"],
			"@public/*": ["./public/*"],
			"@minio/*": ["./minio/*"]
		},
		"plugins": [
			{
				"name": "next"
			}
		],
		"strictNullChecks": true
	},
	"include": [
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx",
		".next/types/**/*.ts",
		"app/layout.jsx",
		"app/providers.jsx",
		"app/(medibook)/layout.jsx",
		"components/medibook/SubMenu.jsx",
		"app/(medibook)/medibook/(Root)/announcements/[announcementId]/page.jsx",
		"app/(medibook)/medibook/(Root)/announcements/page.jsx",
		"app/(website)/signup/signup.server.jsx",
		"app/(medibook)/medibook/sessions/create-session.server.js",
		"app/(medibook)/medibook/sessions/[sessionNumber]/departments/page.jsx",
		"app/(medibook)/medibook/(Root)/announcements/create-announcement.server.js",
		"app/(medibook)/medibook/sessions/[sessionNumber]/departments/[departmentId]/announcements/delete-announcement.server.js",
		"app/(medibook)/medibook/sessions/[sessionNumber]/committees/announcements/delete-announcement.server.js",
		"app/(medibook)/medibook/sessions/[sessionNumber]/committees/add-committee.server.js",
		"components/medibook/CommandMenu.jsx",
		"app/(medibook)/medibook/account/get-user.js",
		"app/(website)/announcements/page.jsx"
	],
	"exclude": ["node_modules"]
}
